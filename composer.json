{
  "name": "coresender/coresender-sdk-php",
  "description": "The Coresender PHP SDK.",
  "require": {
    "php": "^7.1",
    "php-http/client-common": "^2.1",
    "php-http/discovery": "^1.0",
    "psr/log": "^1.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "nyholm/psr7": "^1.2",
    "symfony/var-dumper": "^5.0",
    "friendsofphp/php-cs-fixer": "^2.16",
    "kriswallsmith/buzz": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Coresender\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Coresender\\Tests\\": "tests/"
    }
  },
  "license": "MIT",
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
